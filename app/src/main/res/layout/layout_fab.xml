<?xml version="1.0" encoding="utf-8"?>

<com.gf.movie.reminder.ui.ExpandableFab xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/expandable_fab"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentBottom="true"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_gravity="bottom|end"
    fab:fab="@+id/fab"
    fab:fadeOnFabClick="true">

    <com.gf.movie.reminder.ui.Fab
        android:id="@+id/fab"
        android:layout_width="@dimen/fab_size"
        android:layout_height="@dimen/fab_size"
        android:layout_gravity="bottom|end"
        android:background="@drawable/bg_fab"
        android:src="@drawable/ic_plus_holo_dark" />

</com.gf.movie.reminder.ui.ExpandableFab>